  <body>

   <h1>Create a new user</h1>
    <form action ="/users" method='POST'>
      <label>
        Email:
          <input name = 'email' />
      </label>
      <label>
        First name:
          <input name = 'firstName' />
      </label>
      <label>
        Last Name:
          <input name = 'lastName' />
      </label>
      <label>
        Date of Birth:
          <input type = 'date' name = 'dob' />
      </label>
      <button type='submit'>submit</button>
  </form>
  </body>
</html>
